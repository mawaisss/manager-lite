<h2>Attendance</h2>
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Employee</th>
      <th>Status</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% attendance.forEach(att => { %>
      <tr>
        <td><%= att.date %></td>
        <td><%= att.employee %></td>
        <td><%= att.status %></td>
        <td><a href="#">View</a></td>
      </tr>
    <% }) %>
  </tbody>
</table>
