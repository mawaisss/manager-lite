<h2>Meeting Notes</h2>
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Subject</th>
      <th>Notes</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% meetings.forEach(meeting => { %>
      <tr>
        <td><%= meeting.date %></td>
        <td><%= meeting.subject %></td>
        <td><%= meeting.notes %></td>
        <td><a href="#">View</a></td>
      </tr>
    <% }) %>
  </tbody>
</table>
