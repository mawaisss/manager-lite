<h2>Expenses & Income</h2>
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Category</th>
      <th>Amount</th>
      <th>Notes</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% expenses.forEach(exp => { %>
      <tr>
        <td><%= exp.date %></td>
        <td><%= exp.category %></td>
        <td><%= exp.amount %></td>
        <td><%= exp.notes %></td>
        <td><a href="#">View</a></td>
      </tr>
    <% }) %>
  </tbody>
</table>
